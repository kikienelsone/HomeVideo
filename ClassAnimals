public class Main {
    public static void main(String[] args) {
     Cat barsik = new Cat("Барсик",4 );
     Bird kesha = new Bird("Кеша", 2);
     barsik.run();
     kesha.fly();
    }}

abstract class Animal{
    String nickname;
    int age;
    public Animal(String nickname, int age) {
        this.nickname = nickname;
        this.age = age;
    }}

class  Cat extends Animal{
    public Cat(String nickname, int age) {
        super(nickname, age);}
        void run(){
            System.out.println("животное "+ this.nickname+",бежит");
    }}

class Bird extends Animal{
    public Bird(String nickname, int age) {
        super(nickname, age);}
    void fly() {
        System.out.println("животное " + this.nickname + ", летит");
    }}
