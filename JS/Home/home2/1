let num1 = +prompt()
let num2 = +prompt()
let num3 = +prompt()

res = null

if((num2 < num1 && num1 < num3) || (num3 < num1 && num1 < num2)) {
    res = num1
}
else if((num1 < num2 && num2<num3) || (num3<num2 && num2<num1)) {
    res = num2
}
else if((num1<num3 && num3<num2) || (num2<num3 && num3<num1)) {
    res = num3

}
console.log("среднее число:"+res)
