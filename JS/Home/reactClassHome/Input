import React, {Component} from "react";


class Input extends Component {

constructor(props) {

    super(props);
}



    render() {
        return (
            <div>
                <input type="number" value={this.props.value} onChange={(event)=> this.props.setState({[this.props.keyState]: event.target.value})}></input>

                
                
                </div>
            
        );
    }
}

export default Input;
