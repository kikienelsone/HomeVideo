import java.io.FileReader;

public class CheckkkkkProxy {
    public static void main(String[] args) {
        try {
            FileReader reader = new FileReader("C:\\java\\1.txt");
            int c;
            while ((c = reader.read()) !=-1){
                System.out.print((char)c);
            }
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}
