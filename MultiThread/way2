public class MultiThread2 {
    public static void main(String[] args) {
        Thread t1 = new Thread(new MyThred("лалала"));
        Thread t2 = new Thread(new MyThred("блабла"));
        t1.start();
        t2.start();
    }
}
class MyThred implements Runnable{
    String task;

    public MyThred(String task) {
        this.task = task;
    }

    @Override
    public void run(){
        for (int i = 0; i < 10000; i++) {
            System.out.println(i+" "+task);

        }
    }
}
